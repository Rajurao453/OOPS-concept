
import java.util.*;
public class ArraylistDemo {

	public static void main(String[] args) {
		ArrayList <String> arraylist=new ArrayList <String>();
		arraylist.add("Naveen");
		arraylist.add("Raju");
		arraylist.add("Abhi");
		arraylist.add("Harish");
		System.out.println(arraylist);
		arraylist.remove(1);
		System.out.println(arraylist);


	}

}
